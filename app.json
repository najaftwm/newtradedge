{
  "expo": {
    "name": "TradEdge",
    "slug": "tradedge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "tradedge",
    "deepLinking": true,
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tradedge.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tradedge.app",
      "googleServicesFile": "./android/app/google-services.json",  // Corrected property name
      "versionCode": 1,
      "icon": "./assets/images/logo.png",
      "notification": {
        "icon": "./assets/images/notification-icon.png",
        "color": "#6200EE"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 170,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png", // Optional: Custom notification icon
          "color": "#6200EE", // Optional:  Notification color (Android)
          "sounds": [
            "./assets/sounds/notification.wav" // Optional: Custom notification sound
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0dca21b6-7a9a-4c7d-8da4-7f73d09c3b7e"
      }
    },
    "owner": "najaf1705"
  }
}
